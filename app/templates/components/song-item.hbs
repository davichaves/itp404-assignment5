<div class="reddit">
  <hr>
  <p>Title: {{song.title}}</p>
  <p>Price: {{song.price}}</p>
  <p>playCount: {{song.playCount}}</p>
  {{#if songIsPopular}}
    <img src="http://pix.iemoji.com/images/emoji/apple/ios-9/256/fire.png" height="50" width="50">
  {{/if}}
  {{#if songIsExpensive}}
    <h4>$$</h4>
  {{else}}
    <h4>$</h4>
  {{/if}}
</div>
{{yield}}
